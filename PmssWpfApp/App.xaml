<Application x:Class="PmssWpfApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PmssWpfApp"
             xmlns:localVm="clr-namespace:PmssWpfApp.ViewModel"
             StartupUri="RibbonMainWindow.xaml">
    <Application.Resources>
        <local:RibbonMainViewModel x:Key="MainVm" />
        <localVm:DrawOptionsGroupVm x:Key="DrawOptionsVm" />
        <localVm:MapVm x:Key="MapVm" />
        <localVm:ProductSendVm x:Key="PSendVm"/>
        <localVm:DataBaseVm x:Key="DbVm"/>
        <localVm:ProductMakeVm x:Key="PMakeVm"/>
        <BooleanToVisibilityConverter x:Key="BoolToVis" />
        <localVm:LevelValueManagerVm x:Key="LevelManagerVm" />
        <localVm:DataMonitoringVm x:Key="MonitorVm" />
        <localVm:DataCheckingVm x:Key="CheckingVm" />
        <localVm:MouseInfoVm x:Key="MouseInfoVm" />
    </Application.Resources>
</Application>
