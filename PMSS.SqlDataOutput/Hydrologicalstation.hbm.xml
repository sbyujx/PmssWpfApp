<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PMSS.SqlDataOutput" namespace="PMSS.SqlDataOutput" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Hydrologicalstation" table="hydrologicalstation" lazy="true" >
    <id name="Uid" column="UID" />
    <property name="Name">
      <column name="Name" sql-type="varchar(45)" not-null="true" />
    </property>
    <property name="Longitude">
      <column name="Longitude" sql-type="double" not-null="false" />
    </property>
    <property name="Latitude">
      <column name="Latitude" sql-type="double" not-null="false" />
    </property>
    <property name="River">
      <column name="River" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Hydrographicnet">
      <column name="HydrographicNet" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Basin">
      <column name="Basin" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Administrativeregion">
      <column name="AdministrativeRegion" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Address">
      <column name="Address" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Type">
      <column name="Type" sql-type="varchar(45)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>